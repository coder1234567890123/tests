                {strokeStyle: "#DF542D", min: 62, max: 80}, // Orange
                {strokeStyle: "#DA362B", min: 80, max: 100}  // Red
            ],

        };
        var target = document.getElementById('riskGauge'); // your canvas element
        var gauge = new Gauge(target).setOptions(opts); // create sexy gauge!
