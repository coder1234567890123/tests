            background-image: -ms-linear-gradient(-180deg, #000000 50%, transparent 50%);
            background-image: -o-linear-gradient(-180deg, #000000 50%, transparent 50%);
            width: 368px;
            height: 368px;
            overflow: hidden;
            z-index: 9999;
            position: absolute;
            top: 88px;
